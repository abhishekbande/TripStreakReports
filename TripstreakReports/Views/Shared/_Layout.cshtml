    $(window).load(function() {
        $("#overlay").fadeOut("slow");
    });
        
    function ApplyPagingSortingSelectionToTable() {
        table = $('#Amenities').DataTable();
        $('#Amenities tbody').on('click', 'tr', function() {
            if ($(this).hasClass('bg')) {
                $(this).removeClass('bg');
            } else {
                //table.$('tr.bg').removeClass('bg');
                $(this).addClass('bg');
            }
        });
    }


    function updateSuccess(data) {
        alert("Success");
        RemoveMixpanelSpinner("PreconfiguredReportOverlay");
    }

    function onComplete() {
        alert("Complete");
        RemoveMixpanelSpinner("PreconfiguredReportOverlay");
    }

    function LoadCustomReportPanelDropdown() {
        
        $('#OutputColumns').multiselect({
            includeSelectAllOption: true,
            enableFiltering: true,
            numberDisplayed: 0,
            enableCaseInsensitiveFiltering: true,
            maxHeight: 200,
            buttonWidth: '100%',
            textalign: 'left',
            dropRight: true
        });
        $(".caret").css('float', 'right');
        $(".caret").css('margin-top', '8px');
        $(".caret").css('margin-right', '-7px');      

        $('.multiselect', this.$container).css({
            'text-align': 'left'
        });
    }

    function AddSnippner() {
        $("#OverlayAmenityTable").addClass("snipper1");
        $("#Amenities").addClass("fadeout");
    }
       
